<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>netstandard2.0;netcoreapp3.0;netcoreapp3.1;</TargetFrameworks>
	  <Product>FreeSql.PgSql.Natasha.Extension</Product>
	  <Description>基于 Freesql 和 Natasha 的 SQL ORM扩展。</Description>
	  <Copyright>NMSAzulX</Copyright>
	  <PackageId>NMS.Aries</PackageId>
	  <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
	  <PackageReleaseNotes>正式版  使用Freesql 1.10.1 支持 netcore3.1 runtime</PackageReleaseNotes>
	  <Version>1.5.0.1</Version>
	  <PackageLicenseUrl>https://github.com/night-moon-studio/Aries/blob/master/LICENSE</PackageLicenseUrl>
	  <PackageProjectUrl>https://github.com/night-moon-studio/Aries</PackageProjectUrl>
	  <PackageTags>Natasha;Freesql;Pgsql;Aries</PackageTags>
	  <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	  <Company>NMSAzulX</Company>
	  <PackageIconUrl>https://avatars2.githubusercontent.com/u/51699821</PackageIconUrl>
	  <Authors>NMSAzulX</Authors>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="DotNetCore.Natasha.CSharp.All" Version="2.0.1.1" />
    <PackageReference Include="FreeSql" Version="1.10.3" />
    <PackageReference Include="NMS.DynamicCache" Version="1.4.2.2" />
  </ItemGroup>


</Project>
