<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFrameworks>netcoreapp3.1;netcoreapp3.0;net5.0;</TargetFrameworks>
		<OutputType>Library</OutputType>
		<Product>Aries.Web</Product>
		<Description>Aries 的 Web 版本</Description>
		<Copyright>NMSAzulX</Copyright>
		<PackageId>NMS.Aries.Web</PackageId>
		<GeneratePackageOnBuild>False</GeneratePackageOnBuild>
		<PackageReleaseNotes>正式发布</PackageReleaseNotes>
		<Version>2.0.0</Version>
		<PackageLicenseUrl>https://github.com/night-moon-studio/Aries/blob/master/LICENSE</PackageLicenseUrl>
		<PackageProjectUrl>https://github.com/night-moon-studio/Aries</PackageProjectUrl>
		<PackageTags>Natasha;Freesql;Web;Aries;NMS;</PackageTags>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<Company>AzulX</Company>
		<PackageIconUrl>https://avatars2.githubusercontent.com/u/51699821</PackageIconUrl>
		<Authors>AzulX</Authors>
		<IsPackable>true</IsPackable>
		<FileVersion>2.0.0.0</FileVersion>
		<AssemblyVersion>2.0.0.0</AssemblyVersion>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|netcoreapp3.1|AnyCPU'">
	  <DocumentationFile>Aries.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Aries\Aries.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="Aries.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
	</ItemGroup>



</Project>
